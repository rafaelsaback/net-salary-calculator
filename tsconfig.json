{
  "compilerOptions": {
    "target": "es2015",
    "module": "amd",
    "sourceMap": true,
    "outDir": "./build",
    "rootDir": "./components/typescript/",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true
  },
  "exclude": [
      "node_modules"
  ]
}
